: url(../media/static/image-header.jpg);\n        background-position: center;\n        background-size: cover;\n        padding: 15px 40px;\n\n        .header{\n            display:flex;\n            flex-direction: row;\n            flex-wrap: wrap;\n            justify-content: space-between;\n            align-items: center;\n            .menu-bar ul{\n                display: flex;\n                flex-direction: row;\n                justify-content: space-between;\n                align-items: center;\n                min-height: 70px;\n                min-width: 400px;\n                li{\n                    // cursor: pointer;\n                    height: 40px;\n                    width: 60px;\n                    \n                    line-height: 40px;\n                    a{\n                        font-size: 20px;\n                        color: white;\n                    }\n                    &:last-child{\n                        background: white;\n                        border-radius: 30px;\n                        width: 120px;\n                        text-align: center;\n                        cursor: pointer;\n                        transition: 0.5s;\n                        a{\n                            color: black;\n                        }\n                    }\n                    &:last-child:hover{\n                        background: rgb(255, 241, 223);\n                    }\n                }\n            }\n\n        }\n        .main-layout-content{\n            margin-top: 12.5%;\n            color: white;\n            font-weight: bold;\n            text-transform: uppercase;\n            font-size: 75px;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            justify-content: center;\n            img{\n                margin-top: 65px;\n                animation: mover 1s infinite alternate;\n            }\n            @keyframes mover{\n                0%{\n                    transform: translateY(10px);\n                }\n                100%{\n                    transform: translateY(-30px);\n                }\n            }\n        }\n\n    }\n    .section-content{\n        display: flex;\n        flex-direction: row;\n        height: $container-height;\n        flex-wrap: nowrap;\n        .container{\n            position: relative;\n            display: flex;\n            width: 50%;\n            .container-l{\n                margin: auto auto;\n                width: 70%;\n            }\n            h1{\n                font-size: 40px;\n                font-weight: bold;\n                margin-bottom: 18px;\n            }\n            p{\n                color: #848391;\n                font-size: 15px;\n                line-height: 1.9;\n                margin-bottom: 18px;\n            }\n            .button-img{\n                width: 150px;\n            }\n        }\n        .side{\n            img{\n                width: 100%;\n                height: 100%;\n            }\n        }\n        .section-context{\n            position: absolute;\n            bottom: 5%;\n            left: 50%;\n            transform: translateX(-50%);\n            h1{\n                color: #005d4e;\n            }\n        }\n        \n    }\n    .testimonials{\n        h1{\n            text-align: center;\n            margin: 100px;\n            font-size: 40px;\n            font-family: 'Fraunces', serif;\n            text-transform: uppercase;\n            color: hsl(210, 4%, 67%);\n        }\n    }\n}"},"lineCount":null}},"error":null,"hash":"a0be94125d636bb30989363c19a3f0cf","cacheData":{"env":{}}}